<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" internalLogFile="nlog_log.txt">
  <targets>
    <target name="log_file" xsi:type="File" fileName="LOG.txt"/>
  </targets>
  <rules>
    <logger name="*" minlevel="Debug" writeTo="log_file" />
  </rules>
</nlog>